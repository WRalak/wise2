{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/components/Footer.jsx", "app/components/Header.jsx", "app/components/LoginForm.jsx", "app/components/mainFooter.jsx", "app/components/MainSection.jsx", "app/components/MainTwo.jsx", "app/components/Resources.jsx", "app/components/Section.jsx", "app/components/Testimonials.jsx", "app/services/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
